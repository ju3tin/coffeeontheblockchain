{
  "expo": {
    "name": "my-app1",
    "slug": "coffeeontheblockchain",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "packagerOpts": {"config": "rn-cli.config.js"},
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e21e8d71-f00e-4b95-bc96-66025b999efb"
      }
    },
    "owner": "ju3t1n"
  }
}
